{
  "name": "huayra",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon app.js",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "standard",
    "eject": "react-scripts eject",
    "format": "prettier --single-quote --trailing-comma=all --write \"src/**/*.js\""
  },
  "author": "RedOneLai",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "proxy": "http://localhost:3001/",
  "standard": {
    "globals": [
      "fetch",
      "localStorage",
      "Headers",
      "$",
      "callback"
    ],
    "ignore": [
      "/scripts/**"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      ".*\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "async": "1.x.x",
    "bcrypt": "*",
    "body-parser": "1.x.x",
    "classnames": "^2.2.5",
    "compression": "1.x.x",
    "connect-mongo": "1.x.x",
    "cookie-parser": "1.x.x",
    "csurf": "1.x.x",
    "dotenv": "^4.0.0",
    "emailjs": "1.x.x",
    "express": "4.13.x",
    "express-session": "1.x.x",
    "helmet": "2.x.x",
    "jade": "1.x.x",
    "method-override": "2.3.10",
    "mongoose": "4.x.x",
    "morgan": "1.x.x",
    "object-assign": "^4.1.1",
    "passport": "0.3.x",
    "passport-local": "1.x.x",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.0.0",
    "react-table": "^6.8.6",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "serve-static": "1.10.x"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "font-awesome": "4.x.x",
    "grunt": "1.x.x",
    "grunt-cli": "1.x.x",
    "grunt-concurrent": "2.x.x",
    "grunt-contrib-clean": "1.x.x",
    "grunt-contrib-copy": "1.x.x",
    "grunt-contrib-jshint": "1.x.x",
    "grunt-contrib-less": "1.x.x",
    "grunt-contrib-uglify": "1.x.x",
    "grunt-contrib-watch": "1.x.x",
    "grunt-newer": "1.x.x",
    "grunt-nodemon": "0.4.x",
    "html5shiv": "3.x.x",
    "moment": "2.19.x",
    "react-scripts": "^1.0.11",
    "respond.js": "1.x.x",
    "standard": "*"
  }
}
